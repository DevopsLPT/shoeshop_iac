services:
  jfrog:
    image: releases-docker.jfrog.io/jfrog/artifactory-oss:7.77.5
    container_name: jfrog
    ports:
      - "8081:8081" 
      - "8082:8082" 
    volumes:
      - /data/jfrog/artifactory_data:/var/opt/jfrog/artifactory
    restart: unless-stopped
    networks:
      - PrviNetwork

networks:
  PrviNetwork:
    external: true


